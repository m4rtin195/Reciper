<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar"> <!--Theme.AppCompat.Light.NoActionBar-->
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>


        <item name="android:textColor">#111111</item>


    </style>

    <style name="AppTheme.Launcher">
        <item name="android:windowBackground">@drawable/launch_screen</item>
    </style>

    <style name="CustomPreferenceCategory" parent="Preference.Category.Material">
        <item name="iconSpaceReserved">false</item>
    </style>
    <style name="CustomPreference" parent="Preference.Material">
        <item name="iconSpaceReserved">false</item>
    </style>

    <style name="Toolbar" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />


</resources>